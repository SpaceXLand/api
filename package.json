{
  "name": "spacex-graphql-api",
  "version": "1.0.0",
  "description": "Open Source GraphQL API for rocket, core, capsule, pad, and launch data",
  "main": "./src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "dev": "nodemon",
    "generate": "gql-gen",
    "precommit": "graphql-inspector introspect src/schema/index.ts --write schema.graphql --require ts-node/register/transpile-only && git add schema.graphql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spacexland/api"
  },
  "keywords": [
    "graphql",
    "typescript",
    "spacex",
    "mongodb"
  ],
  "author": "Carlos Rufo",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/spacexland/api/issues"
  },
  "homepage": "https://github.com/spacexland/api",
  "graphql-inspector": {
    "diff": true,
    "schema": {
      "ref": "master",
      "path": "schema.graphql"
    }
  },
  "dependencies": {
    "apollo-server-express": "^2.3.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "graphql": "^14.0.2",
    "graphql-depth-limit": "^1.1.0",
    "graphql-rate-limit": "^1.2.2",
    "graphql-tag": "^2.10.0",
    "graphql-validation-complexity": "^0.2.4",
    "lodash": "^4.17.11",
    "lower-case": "^1.1.4",
    "moment": "^2.23.0",
    "mongodb": "^3.1.10",
    "sofa-api": "0.3.0",
    "swagger-ui-express": "^4.0.2",
    "ts-node": "^8.0.0",
    "typescript": "^3.2.1"
  },
  "devDependencies": {
    "@graphql-inspector/cli": "1.18.1",
    "@types/express": "4.16.1",
    "babel-runtime": "6.26.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-mongodb": "1.0.0",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-security": "1.4.0",
    "graphql-code-generator": "0.18.1",
    "graphql-codegen-typescript-common": "0.18.1",
    "graphql-codegen-typescript-resolvers": "0.18.1",
    "graphql-codegen-typescript-server": "0.18.1",
    "husky": "2.1.0",
    "nodemon": "1.18.11"
  }
}
